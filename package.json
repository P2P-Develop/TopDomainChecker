{
    "name": "tld-checker",
    "version": "3.0.0",
    "description": "Brute-force the top-level domains with parallel.",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "build:linux32": "nexe --build -t linux-x86-12.18.4 --verbose -o dist/linux-32/tldcheck",
        "build:linux": "nexe --build -t linux-x64-12.18.4 --verbose -o dist/linux-64/tldcheck",
        "build-win": "nexe --build -t windows-x64-12.18.4 --verbose -o dist/win/tldcheck",
        "build-mac": "nexe --build -t mac-x64-12.18.4 --verbose -o dist/mac/tldcheck",
        "lint": "npm-run-all -p lint:*",
        "lint:js": "eslint .",
        "lint:md": "remark .",
        "fix": "eslint --fix ."
    },
    "bin" : {
        "tldcheck": "src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/P2P-Develop/TopDomainChecker.git"
    },
    "author": "P2P-Develop",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/P2P-Develop/TopDomainChecker/issues"
    },
    "homepage": "https://github.com/P2P-Develop/TopDomainChecker#readme",
    "dependencies": {
        "boxen": "^4.2.0",
        "chalk": "^4.1.0",
        "cli-cursor": "^3.1.0",
        "command-line-args": "^5.1.1",
        "command-line-usage": "^6.1.0",
        "copy-paste": "^1.3.0",
        "enquirer": "^2.3.6",
        "figures": "^3.2.0",
        "get-stdin": "^8.0.0",
        "https": "^1.0.0",
        "ora": "^5.1.0",
        "ping": "^0.2.3",
        "pretty-error": "^2.1.1",
        "readline": "^1.3.0",
        "terminal-link": "^2.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/eslint-parser": "^7.11.5",
        "@babel/plugin-syntax-top-level-await": "^7.10.4",
        "eslint": "^7.10.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-unicorn": "^22.0.0",
        "eslint-plugin-yaml": "^0.3.0",
        "nexe": "^4.0.0-beta.14",
        "npm-run-all": "^4.1.5",
        "remark-cli": "^8.0.1",
        "remark-lint": "^7.0.1",
        "remark-lint-heading-whitespace": "^1.0.0",
        "remark-lint-match-punctuation": "^0.2.0",
        "remark-lint-no-dead-urls": "^1.1.0",
        "remark-lint-no-empty-sections": "^4.0.0",
        "remark-lint-no-url-trailing-slash": "^3.0.1",
        "remark-preset-lint-recommended": "^4.0.1"
    }
}
