{
    "name": "tld-checker",
    "version": "5.0.0",
    "description": "Brute-force the top-level domains with parallel.",
    "keywords": [
        "parallel",
        "topleveldomains",
        "tld",
        "brute-force",
        "ping",
        "http",
        "https",
        "iana",
        "topdomainchecker",
        "tldcheck",
        "tld-checker",
        "domains",
        "human-friendly"
    ],
    "homepage": "https://github.com/P2P-Develop/TopDomainChecker#readme",
    "bugs": {
        "url": "https://github.com/P2P-Develop/TopDomainChecker/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/P2P-Develop/TopDomainChecker.git"
    },
    "license": "MIT",
    "author": "Potato1682 <contact@potato1682.ml> (https://www.potato1682.ml)",
    "main": "src/api.ts",
    "types": "dist/src",
    "bin": {
        "tldcheck": "dist/src/index.js"
    },
    "files": [
        "dist/src"
    ],
    "scripts": {
        "build": "tsc -p . && cp -r src/locales dist/src",
        "clean": "rimraf dist",
        "fix": "eslint --fix .",
        "preinstall": "npx only-allow pnpm",
        "lint": "npm-run-all -p lint:*",
        "lint:js": "eslint .",
        "lint:md": "remark -r .remarkrc.js .",
        "release": "npm run build && np",
        "start": "node --require ts-node/register src/index.ts",
        "test": "jest --coverage",
        "posttest": "npm run lint",
        "typedoc": "typedoc --out typedoc src"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.ts": [
            "eslint --fix",
            "git add"
        ],
        "package.json": [
            "sort-package-json"
        ]
    },
    "dependencies": {
        "boxen": "^5.0.0",
        "chalk": "^4.1.0",
        "cli-cursor": "^3.1.0",
        "cli-truncate": "^2.1.0",
        "command-line-args": "^5.1.1",
        "command-line-usage": "^6.1.1",
        "copy-paste": "^1.3.0",
        "enquirer": "^2.3.6",
        "figures": "^3.2.0",
        "get-stdin": "^8.0.0",
        "got": "^11.8.1",
        "https": "^1.0.0",
        "i18n": "^0.13.2",
        "ora": "^5.3.0",
        "ping": "^0.4.0",
        "pretty-error": "^3.0.3",
        "readline": "^1.3.0",
        "sudo-block": "^3.0.0",
        "terminal-link": "^2.1.1",
        "tslib": "^2.1.0",
        "undici": "^3.2.0"
    },
    "devDependencies": {
        "@types/command-line-args": "^5.0.0",
        "@types/command-line-usage": "^5.0.1",
        "@types/copy-paste": "^1.1.30",
        "@types/i18n": "^0.12.0",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.22",
        "@types/ping": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.18.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-unicorn": "^27.0.0",
        "eslint-plugin-yaml": "^0.3.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "jest-github-actions-reporter": "^1.0.3",
        "lint-staged": "^10.5.3",
        "npm-run-all": "^4.1.5",
        "remark-cli": "^9.0.0",
        "remark-lint": "^8.0.0",
        "remark-lint-blank-lines-1-0-2": "^3.0.0",
        "remark-lint-heading-whitespace": "^1.0.0",
        "remark-lint-match-punctuation": "^0.2.0",
        "remark-lint-mdash-style": "^1.1.1",
        "remark-lint-no-chinese-punctuation-in-number": "^0.1.1",
        "remark-lint-no-dead-urls": "^1.1.0",
        "remark-lint-no-empty-sections": "^4.0.0",
        "remark-lint-no-url-trailing-slash": "^3.0.1",
        "remark-lint-spaces-around-word": "^0.1.1",
        "remark-preset-lint-recommended": "^5.0.0",
        "rimraf": "^3.0.2",
        "sort-package-json": "^1.48.1",
        "ts-jest": "^26.5.0",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.19",
        "typescript": "^4.1.3"
    },
    "engines": {
        "pnpm": ">= 3.4.0"
    },
    "publishConfig": {
        "main": "dist/src/api.js",
        "types": "dist/src/api.d.ts"
    }
}
